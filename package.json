{
  "name": "babel-plugin-watcher",
  "version": "0.0.1",
  "description": "Babel plugin to spy on variables",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davecoates/babel-plugin-watcher.git"
  },
  "author": "Dave Coates",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davecoates/babel-plugin-watcher/issues"
  },
  "homepage": "https://github.com/davecoates/babel-plugin-watcher#readme",
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.2.0",
    "eslint": "^1.10.3",
    "mocha": "^2.2.5",
    "rimraf": "^2.4.3",
    "rollup-plugin-node-builtins": "^1.0.7"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src -d lib && node ./build/runtime.js",
    "build:watch": "node ./build/runtime.js && babel src -d lib --watch",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "npm run test -- --watch",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "babel-plugin",
    "dx"
  ],
  "dependencies": {
    "lodash": "^4.6.1",
    "rollup": "^0.34.4",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "touch": "^1.0.0",
    "value-mirror": "0.0.4",
    "ws": "^1.0.1"
  }
}
